    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "mocha": "^5.1.1",
    "nyc": "^11.8.0",
    "serverless-offline": "^3.21.1",
    "serverless-webpack": "^5.1.0",
    "should": "^13.2.1",
    "sinon": "^5.0.7",
    "sort-object-keys": "^1.1.2",
    "webpack": "^3.3.0",
    "webpack-node-externals": "^1.6.0",
    "write-json-file": "^2.3.0"
  },
